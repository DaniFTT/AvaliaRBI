@using System.Globalization;
@using AvaliaRBI._2___Application;
@using AvaliaRBI._3___Domain.Models;
@using AvaliaRBI._3___Domain;



<MudGrid Style="padding-bottom:0rem; width: 730px; height:100%;  font-family: Roboto, Helvetica, Arial, " \sans-serif"\" id="perfomance-grid">

        <style>

            #perfomance-grid .mud-paper {
                color: rgb(42 42 52);
                background-color: transparent;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            #perfomance-grid .mud-table-sticky-header * .mud-table-root .mud-table-head * .mud-table-cell {
                background-color: rgb(255 255 255);
                color: rgb(42 42 52) !important;
                position: sticky;
                z-index: 2;
                top: 0;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            #perfomance-grid .mud-table-root .mud-table-head .mud-table-cell {
                color: rgb(42 42 52);
                font-weight: 500;
                line-height: 1.5rem;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            #perfomance-grid .mud-table-body .mud-table-cell.mud-table-cell-custom-group {
                text-align: center;
                background-color: #fcfcfc;
                color: rgb(42 42 52 / 78%);
                font-weight: 600;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            #perfomance-grid .mud-table-cell-custom-group {
                font-weight: 500;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            #perfomance-grid .mud-table {
                color: rgb(42 42 52);
                background-color: transparent;
                border-radius: var(--mud-default-borderradius);
                transition: unset;
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

            * {
                font-family: Roboto, Helvetica, Arial, "sans-serif"
            }

        </style>


        <MudItem xs="12" Style="padding-bottom: 1rem;     padding-top: 0;">

            <div class="d-flex flex-grow-1 gap-3">
                <MudPaper Style="height: 4rem;     width: 25%;" Elevation="4">
                    <MudGrid Style="align-items: center;">
                        <MudItem xs="2">
                            <svg width="34" height="32" viewBox="0 0 34 32" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                <rect width="34" height="32" fill="url(#pattern0)" />
                                <defs>
                                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                        <use xlink:href="#image0_1_4" transform="scale(0.0123457 0.0125)" />
                                    </pattern>
                                    <image id="image0_1_4" width="219" height="80" xlink:href="data:image/png;base64," />
                                </defs>
                            </svg>
                        </MudItem>
                        <MudItem xs="9" Style="margin-left: 5px;">
                        <MudText Typo="Typo.body2" Style=" font-family: Roboto, Helvetica, Arial">RBI Papéis</MudText>
                        </MudItem>
                    </MudGrid>
                </MudPaper>
            <MudPaper Style="height: 4rem;     width: 50%;     text-align: center" Elevation="4">
                <MudText Typo="Typo.h6" Style="font-family: Roboto, Helvetica, Arial">Avaliação Mensal</MudText>
                </MudPaper>
            <MudPaper Style="height: 4rem;     width: 25%;     text-align: right;" Elevation="4">
                <MudText Typo="Typo.body2" Style="font-family: Roboto, Helvetica, Arial">Concluido em @(assessment.ClosedDate.HasValue ? assessment.ClosedDate.Value.Date.ToString("dd/MM/yyyy") : string.Empty)</MudText>

                    </MudPaper>
                </div>

            </MudItem>
        <MudItem xs="6" Style="padding-bottom: 0;     padding-top: 0;width: 100%">
                <MudCard Style="height: 4rem;" Elevation="4">
                    <MudCardContent Style="padding: 12px;">
                    <MudText Typo="Typo.body2" Style="font-size: 0.8rem;  font-family: Roboto, Helvetica, Arial">Funcionário</MudText>
                    <MudText Style="font-size: 1rem;  font-family: Roboto, Helvetica, Arial">@employee.Name</MudText>
                    </MudCardContent>
                </MudCard>
            </MudItem>
        <MudItem xs="6" Style="padding-bottom: 0;     padding-top: 0;width: 100%">
            <MudCard Style="height: 4rem;" Elevation="4">
                <MudCardContent Style="padding: 12px;">
                    <MudText Typo="Typo.body2" Style="font-size: 0.8rem;  font-family: Roboto, Helvetica, Arial">RG</MudText>
                    <MudText Style="font-size: 1rem;  font-family: Roboto, Helvetica, Arial">@employee.RG</MudText>
                </MudCardContent>
            </MudCard>
        </MudItem>
@*        <MudItem xs="6" Style="padding-bottom: 0;     padding-top: 0;width: 100%">
                <MudCard Style="height: 4rem;" Elevation="4">
                    <MudCardContent Style="padding: 12px;">
                    <MudText Typo="Typo.body2" Style="font-size: 0.8rem;  font-family: Roboto, Helvetica, Arial">Competência</MudText>
                    <MudText Style="font-size: 1rem;  font-family: Roboto, Helvetica, Arial">@(assessment.ReferenceDate.HasValue ? GetFormatedDate(assessment.ReferenceDate.Value) : string.Empty)</MudText>
                    </MudCardContent>
                </MudCard>
            </MudItem>*@
        <MudItem xs="6" Style="padding-bottom: 0;     padding-top: 0;width: 100%">
                <MudCard Style="height: 4rem;" Elevation="4">
                    <MudCardContent Style="padding: 12px;">
                    <MudText Typo="Typo.body2" Style="font-size: 0.8rem;  font-family: Roboto, Helvetica, Arial">Cargo</MudText>
                    <MudText Style="font-size: 1rem;  font-family: Roboto, Helvetica, Arial">@employee.Position.Name</MudText>
                    </MudCardContent>
                </MudCard>
            </MudItem>
        <MudItem xs="6" Style="padding-bottom: 0;     padding-top: 0; width: 100%">
                <MudCard Style="height: 4rem;" Elevation="4">
                    <MudCardContent Style="padding: 12px;">
                    <MudText Typo="Typo.body2" Style="font-size: 0.8rem;  font-family: Roboto, Helvetica, Arial">Departamento</MudText>
                    <MudText Style="font-size: 1rem;  font-family: Roboto, Helvetica, Arial">@employee.Position.Department.Name</MudText>
                    </MudCardContent>
                </MudCard>
            </MudItem>


            <MudDivider DividerType="DividerType.Middle" Class="my-1" Style="width: 690px; color: rgb(42 42 52 / 30%)" />

        <MudItem xs="12" Style="padding-bottom: 0;     padding-top: 0; width: 100%">

            <MudGrid Style="height: 100%; width: 100%">
                    <MudItem xs="12">
                    <MudText Style="padding-top: 1rem;padding-left: 1rem; padding-bottom: 12px; color: rgb(42 42 52);  font-family: Roboto, Helvetica, Arial" Typo="Typo.h6">Resultado</MudText>
                    </MudItem>
                <MudItem xs="4" Style="padding-bottom: 0; padding-top: 0; width: 100%;">
                    <MudCard Style="width: 100%;" Elevation="4">
                        <MudCardContent>
                            <MudGrid Style="padding-bottom: 2rem;">
                                <MudItem xs="12" Style="padding-bottom: 5px; padding-top: 0;">
                                    <MudText Typo="Typo.body2" Style="font-size: 0.9rem;   font-family: Roboto, Helvetica, Arial">Competência</MudText>
                                </MudItem>
                                <MudItem xs="12" Style="padding-top: 0;     padding-bottom: 0;">
                                    <MudText Typo="Typo.h5" Style=" font-family: Roboto, Helvetica, Arial">@(assessment.ReferenceDate.HasValue ? GetFormatedDate(assessment.ReferenceDate.Value) : string.Empty)</MudText>
                                </MudItem>
                            </MudGrid>
                        </MudCardContent>
                    </MudCard>
                </MudItem>
                <MudItem xs="4" Style="padding-bottom: 0; padding-top: 0; width: 100%;">
                    <MudCard Style="width: 100%;" Elevation="4">
                            <MudCardContent>
                                <MudGrid Style="padding-bottom: 2rem;">
                                <MudItem xs="12" Style="padding-bottom: 5px; padding-top: 0;">
                                    <MudText Typo="Typo.body2" Style="font-size: 0.9rem;   font-family: Roboto, Helvetica, Arial">Nota Final</MudText>
                                    </MudItem>
                                    <MudItem xs="12" Style="padding-top: 0;     padding-bottom: 0;">
                                    <MudText Typo="Typo.h5" Style=" font-family: Roboto, Helvetica, Arial">@assessmentEmployee.Note.ToString("F2")</MudText>
                                    </MudItem>
                                </MudGrid>
                            </MudCardContent>
                        </MudCard>
                    </MudItem>

                <MudItem xs="4" Style="padding-bottom: 0; padding-top: 0; padding-left:4rem; width: 100%;">
                    <MudCard Style="width: 100%;" Elevation="4">
                        <MudCardContent>
                                <MudGrid Style="padding-bottom: 2rem;">
                                    <MudItem xs="12" Style="padding-bottom: 5px; padding-top: 0;">
                                    <MudText Typo="Typo.body2" Style="font-size: 0.9rem;   font-family: Roboto, Helvetica, Arial">Desempenho</MudText>
                                    </MudItem>
                                    <MudItem xs="12" Style="padding-top: 0;     padding-bottom: 0;">
                                    <MudText Typo="Typo.h5" Style=" font-family: Roboto, Helvetica, Arial">@(totalPerfomance.ToString("F2"))%</MudText>
                                </MudItem>
                            </MudGrid>
                        </MudCardContent>
                    </MudCard>
                </MudItem>
            </MudGrid>
        </MudItem>

        <MudDivider DividerType="DividerType.Middle" Class="my-1 mt-3" Style="width:  690px; color: rgb(42 42 52 / 30%)" />

        <MudItem xs="12" Style="    padding-top: 0; padding-bottom: 0">
            <MudCard Elevation="4">
                <MudCardContent Style="    padding-top: 1rem;">
                <MudText Typo="Typo.h6" Style=" font-family: Roboto, Helvetica, Arial">Resumo do Desempenho</MudText>
                    <MudTable Style="padding-top: 10px; max-height: 65vh;overflow-y: auto;"
                              Elevation="0" Items="@criteria" Dense="true" Hover="true" ReadOnly="true" FixedHeader="true"
                              GroupBy="@_groupDefinition">
                        <ColGroup>
                            <col style="width:40%; padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;" />
                            <col style="width:15%; padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;" />
                            <col style="width:15%; padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;" />
                            <col style="width:15%; padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;" />
                            <col style="width:15%; padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;" />
                        </ColGroup>
                        <HeaderContent>
                            <MudTh Style="padding-bottom:0.3rem;  border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;">Nome</MudTh>
                            <MudTh Style="padding-bottom:0.3rem;  border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;">Valor</MudTh>
                            <MudTh Style="padding-bottom:0.3rem;  border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;">Limite</MudTh>
                            <MudTh Style="padding-bottom:0.3rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;">Tendência</MudTh>
                            <MudTh Style="padding-bottom:0.3rem;  border-bottom: 1px solid rgb(42 42 52 / 22%) !important; background-color: #fff !important; color: rgb(42 42 52) !important;">Desempenho</MudTh>
                        </HeaderContent>
                        <GroupHeaderTemplate>
                            <MudTh Class="mud-table-cell-custom-group" colspan="5" Style="    text-align: center;">@($"{aspects.First(a => a.Id == context.Items.First().AspectId).Name}")</MudTh>
                            </GroupHeaderTemplate>
                            <RowTemplate>
                                <MudTd DataLabel="Name" Style="font-size: 0.9rem; padding-top: 0.2rem; padding-bottom: 0.4rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important;background-color: #fff !important; color: rgb(42 42 52) !important;">@context.Name</MudTd>
                                <MudTd DataLabel="Value" Style="font-size: 0.9rem; padding-top: 0.2rem; padding-bottom: 0.4rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important;background-color: #fff !important; color: rgb(42 42 52) !important;">@context.GetValue()</MudTd>
                                <MudTd DataLabel="LimitValue" Style="font-size: 0.9rem; padding-top: 0.2rem; padding-bottom: 0.4rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important;background-color: #fff !important; color: rgb(42 42 52) !important;">@context.GetLimitValue()</MudTd>
                                <MudTd DataLabel="IsPositive" Style="font-size: 0.9rem; padding-top: 0.2rem; padding-bottom: 0.4rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important;background-color: #fff !important; color: rgb(42 42 52) !important;">@(context.IsPositive ? "Positiva" : "Negativa")</MudTd>
                                <MudTd DataLabel="Perfomance" Style="font-size: 0.9rem; padding-top: 0.2rem; padding-bottom: 0.4rem; border-bottom: 1px solid rgb(42 42 52 / 22%) !important;background-color: #fff !important; color: rgb(42 42 52) !important;">@(context.Performance.ToString("F2") + " %")</MudTd>
                            </RowTemplate>
                        </MudTable>
                    </MudCardContent>
                </MudCard>
            </MudItem>

        </MudGrid>
@code {

    [Parameter]
    public MonthlyAssessment assessment { get; set; } = new MonthlyAssessment();

    [Parameter]
    public Employee employee { get; set; } = new Employee();

    [Parameter]
    public AssessmentEmployee assessmentEmployee { get; set; } = new AssessmentEmployee();

    [Parameter]
    public List<AssessmentCriteria> criteria { get; set; } = new List<AssessmentCriteria>();

    [Parameter]
    public List<AssessmentAspect> aspects { get; set; } = new List<AssessmentAspect>();

    [Parameter]
    public double totalPerfomance { get; set; }

    private TableGroupDefinition<AssessmentCriteria> _groupDefinition = new()
    {
        GroupName = "Aspecto: ",
        Indentation = false,
        Expandable = false,
        Selector = (e) => e.AspectId
    };

    public string GetFormatedDate(DateTime date)
    {
        CultureInfo cultura = new CultureInfo("pt-BR");
        string mes = cultura.TextInfo.ToTitleCase(cultura.DateTimeFormat.GetMonthName(date.Month));
        string ano = date.ToString("yyyy");
        return $"{mes}/{ano}";
    }
}
